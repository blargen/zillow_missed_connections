<div class="container">
  <div class="row">
    <div class="col-sm-5 left">
      <ul class="list-group">
        <li class="list-group-item list-group-item-warning">Expected Headers</li>
        <% @expected_headers.each do |header| %>
          <li class="list-group-item list-group-item-dark"><%= header %></li>
        <% end %>
      </ul>
    </div>
    <div class="col-sm-5 right">
      <ul class="list-group">
        <li class="list-group-item list-group-item-info">Missing Headers</li>
        <% @expected_headers.each do |header| %>
          <% if @missing_headers.include?(header) %>
            <li class="list-group-item list-group-item-danger"><%= header %></li>
          <% else %>
            <li class="list-group-item list-group-item-success"><%= header %></li>
          <% end %>
        <% end %>
      </ul>
    </div>
  </div>
</div>